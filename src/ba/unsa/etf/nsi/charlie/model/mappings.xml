        </set>
        <set name="userrolesById" inverse="true">
            <key>
                <column name="USERID"/>
            </key>
            <one-to-many not-found="ignore" class="ba.unsa.etf.nsi.charlie.model.UserRoleEntity"/>
        </set>
    </class>
</hibernate-mapping>