        </property>
        <property name="emailconfirmed">
            <column name="EMAILCONFIRMED" sql-type="nchar(1)" length="1" not-null="true"/>
        </property>
        <property name="passwordhash">
            <column name="PASSWORDHASH" sql-type="nvarchar2(256)" length="256" not-null="true"/>
